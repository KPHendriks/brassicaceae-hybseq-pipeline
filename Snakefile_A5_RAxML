configfile: "./envs/S1L1M1R.yaml"

rule phyutilities:
    input:
        expand("results/A14b_trimmed_prot/{exon}_NT.fasta", exon=config["exons"])
    output:
        "results/A15_S1L1M1R_RAxML/S1L1M1R_msa.aln"
    shell:
        "java -jar src/phyutility/phyutility.jar "
        "-concat -in {input} -out {output}"
